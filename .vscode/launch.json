{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Fass",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/src/fassRun.js",
			"args": ["${workspaceFolder}/program.fass"]
		},
		{
			"name": "Run file",
			"type": "node",
			"request": "launch",
			"program": "${file}"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "npm start",
			"skipFiles": ["<node_internals>/**"],
			"program": "npm run start",
			"args": ["<", "${workspaceFolder}/program.fass"]
		}
	]
}
