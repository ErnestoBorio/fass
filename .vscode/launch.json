	"version": "0.2.0",
	"configurations": [
		{
			"name": "Main",
			"request": "launch",
			"type": "bun",
			"program": "src/main.js",
			"args": ["src/program.fass"],
			"cwd": "${workspaceFolder}"
		},
		{
			"name": "File",
			"request": "launch",
			"type": "bun",
			"runtimeArgs": ["--watch"],
			"program": "${file}",
			"cwd": "${workspaceFolder}"
		}
	]
}
